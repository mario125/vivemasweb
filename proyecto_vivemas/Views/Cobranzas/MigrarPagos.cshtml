
@{
    Layout = "~/Views/Shared/_LayoutMenu.cshtml";
    ViewBag.Title = "Migraciones";
}

<div class="row">
    <div class="col-12 ">
        <div class="card">
            <div class="card-header">
                Pagos
            </div>
        </div>
        <div class="card-body">
            <div class="form-group">
                <label>Migracion de pagos</label>
                <input type="file" class="form-control" id="inputArchivo"/>
            </div>
            <div class="form-group">
                <button class="form-control" id="subirExcel">Convertir</button>
            </div>
            <pre id="jsonData"></pre>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/Content/xls/xlsx.full.min.js"></script>
    <script src="~/Scripts/cobranzas/migrarPagos.js"></script>
}
